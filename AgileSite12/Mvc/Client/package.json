{
  "name": "page-builder",
  "version": "1.0.0",
  "description": "Page Builder",
  "author": "Kentico",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/types/components.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "files": [
    "dist/"
  ],
  "module": "dist/esm/index.js",
  "outputPaths": {
    "build": "./build",
    "dist": "./dist",
    "dancingGoat": "../Samples/DancingGoat/Kentico/Scripts/builders",
    "learningKit": "../Samples/LearningKit/Kentico/Scripts/builders",
    "mainAppWebComponentsFolder": "../../CMS/CMSScripts/WebComponents"
  },
  "scripts": {
    "build": "npm run build:clean && stencil build && npm run webpack && npm run build:copy && npm run lint",
    "dev": "sd concurrent \"stencil build --dev --watch\" \"npm run webpack-dev\"",
    "build:clean": "del-cli %npm_package_outputPaths_build%",
    "build:copy": "npm run build:copy-fonts && npm run build:copy-components-file && npm run build:copy-to-dancing-goat && npm run build:copy-to-learning-kit && npm run build:copy-components-to-mainapp",
    "build:copy-fonts": "cpx --clean src/builder/assets/styles/generated/*.{eot,svg,ttf,woff} %npm_package_outputPaths_build%",
    "build:copy-components-file": "cpx --clean %npm_package_outputPaths_dist%/components.js %npm_package_outputPaths_build%",
    "build:copy-to-dancing-goat": "cpx --clean %npm_package_outputPaths_build%/**/* %npm_package_outputPaths_dancingGoat%",
    "build:copy-to-learning-kit": "cpx --clean %npm_package_outputPaths_build%/**/* %npm_package_outputPaths_learningKit%",
    "build:copy-components-to-mainapp": "cpx --clean %npm_package_outputPaths_dist%/components.js %npm_package_outputPaths_mainAppWebComponentsFolder% && cpx --clean %npm_package_outputPaths_dist%/components/*.* %npm_package_outputPaths_mainAppWebComponentsFolder%/components",
    "lint": "node .\\node_modules\\tslint\\bin\\tslint --project . --config tslint.json --format stylish --exclude dist/*",
    "webpack": "cross-env NODE_ENV=production webpack --mode=production --progress --hide-modules --config ./webpack.prod.config.js",
    "webpack-dev": "webpack-serve --config ./webpack.dev.config.js",
    "test": "cross-env NODE_ENV=test jest",
    "test-dev": "cross-env NODE_ENV=test jest --watchAll",
    "storybook": "start-storybook -p 9001 -c .storybook -s ./dist",
    "stencil-dev": "stencil build --dev --watch --serve"
  },
  "dependencies": {
    "@shopify/draggable": "1.0.0-beta.8",
    "@stencil/core": "^0.10.8",
    "axios": "^0.19.0",
    "babel-runtime": "^6.26.0",
    "dropzone": "^5.5.1",
    "html5sortable": "^0.9.5",
    "inversify": "^4.13.0",
    "lodash.clonedeep": "^4.5.0",
    "lodash.flatmap": "^4.5.0",
    "lodash.flatten": "^4.4.0",
    "lodash.isequal": "^4.5.0",
    "lodash.mapvalues": "^4.6.0",
    "lodash.omit": "^4.5.0",
    "lodash.pick": "^4.4.0",
    "normalizr": "^3.2.4",
    "query-string": "^5.1.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "reflect-metadata": "^0.1.12",
    "simplebar": "^3.1.5",
    "stickyfilljs": "^2.0.5",
    "uuid": "^3.3.2",
    "vue": "^2.5.16",
    "vue-clickaway": "^2.2.2"
  },
  "devDependencies": {
    "@stencil/less": "0.0.1",
    "@stencil/utils": "0.0.4",
    "@stencil/webpack": "0.0.6",
    "@storybook/addon-options": "^4.0.0-alpha.16",
    "@storybook/vue": "^4.0.0-alpha.16",
    "@types/deep-freeze": "^0.1.1",
    "@types/jest": "^23.3.13",
    "@types/lodash.clonedeep": "^4.5.4",
    "@types/lodash.flatmap": "^4.5.4",
    "@types/lodash.flatten": "^4.4.4",
    "@types/lodash.mapvalues": "^4.6.4",
    "@types/lodash.omit": "^4.5.4",
    "@types/lodash.pick": "^4.4.4",
    "@types/node": "^10.5.2",
    "@types/simplebar": "^2.4.2",
    "@types/uuid": "^3.4.4",
    "@vue/test-utils": "^1.0.0-beta.23",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "cpx": "^1.5.0",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "deep-freeze": "0.0.1",
    "del-cli": "^1.1.0",
    "file-loader": "^1.1.11",
    "hard-source-webpack-plugin": "^0.11.2",
    "jest": "^23.6.0",
    "jest-teamcity-reporter": "^0.9.0",
    "less": "^3.8.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.4.1",
    "optimize-css-assets-webpack-plugin": "^5.0.0",
    "speed-measure-webpack-plugin": "^1.2.2",
    "style-resources-loader": "^1.1.0",
    "ts-jest": "^23.10.5",
    "ts-loader": "^4.4.2",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.6.0",
    "typesafe-actions": "^2.2.0",
    "typescript": "^2.9.2",
    "vue-class-component": "^6.2.0",
    "vue-jest": "^2.6.0",
    "vue-loader": "^15.2.6",
    "vue-property-decorator": "^7.0.0",
    "vue-server-renderer": "^2.5.16",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.16",
    "webpack": "^4.16.2",
    "webpack-bundle-analyzer": "^2.13.1",
    "webpack-cli": "^3.1.0",
    "webpack-extraneous-file-cleanup-plugin": "^2.0.0",
    "webpack-merge": "^4.1.3",
    "webpack-serve": "^2.0.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "vue",
      "ts",
      "tsx",
      "json"
    ],
    "transform": {
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest",
      ".*components.*\\.tsx?$": "<rootDir>/node_modules/@stencil/core/testing/jest.preprocessor.js",
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest",
      ".*": "<rootDir>/node_modules/babel-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/example/"
    ],
    "testResultsProcessor": "jest-teamcity-reporter",
    "coverageDirectory": "<rootDir>/coverage/",
    "coverageReporters": [
      "text",
      "teamcity",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "/__fakes__/"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1",
      "^~/(.*)$": "<rootDir>/test/$1"
    }
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
